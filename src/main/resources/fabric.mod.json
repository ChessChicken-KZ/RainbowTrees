{
  "schemaVersion": 1,
  "id": "rainbowtrees",
  "version": "${version}",

  "name": "Rainbow Trees",
  "description": "A StationAPI mod that adds 16 colour trees to your minecraft world!",
  "authors": [
    "ChessChicken-KZ"
  ],
  "contact": {
    "homepage": "https://github.com/ChessChicken-KZ/RainbowTrees",
    "issues": "https://github.com/ChessChicken-KZ/RainbowTrees/issues",
    "sources": "https://github.com/ChessChicken-KZ/RainbowTrees"
  },

  "license": "MIT",
  "icon": "assets/rainbowtrees/icon.png",

  "entrypoints": {
    "stationapi:event_bus": [
      "kz.chesschicken.rainbowtrees.init.RainbowTreesListener"
    ]
  },

  "depends": {
    "fabricloader": "*",
    "minecraft": "1.0.0-beta.7.3",
    "stationapi":"*"
  }
}
